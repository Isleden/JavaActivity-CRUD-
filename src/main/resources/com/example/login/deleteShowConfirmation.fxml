<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="pnShowDeletePage" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.HelloController">
    <children>
        <Label layoutX="14.0" layoutY="71.0" prefHeight="18.0" prefWidth="331.0" text="Are you sure to delete your show permanently?" textAlignment="CENTER">
            <font>
                <Font name="Bodoni MT Black" size="13.0" />
            </font></Label>
        <Button layoutX="38.0" layoutY="156.0" mnemonicParsing="false" onAction="#deleteShowAffirmative" prefHeight="26.0" prefWidth="129.0" text="Yes" />
        <Button layoutX="168.0" layoutY="156.0" mnemonicParsing="false" onAction="#deleteShowNegative" prefHeight="26.0" prefWidth="130.0" text="No" />
      <Label layoutX="14.0" layoutY="114.0" text="Enter show name to delete:" />
      <TextField fx:id="showNameToDelete" layoutX="168.0" layoutY="110.0" />
    </children>
</AnchorPane>
